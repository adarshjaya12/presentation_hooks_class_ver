{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
	"allowSyntheticDefaultImports": true,
    "allowJs": false,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "**/library-app/*"
  ],
  "compileOnSave": true
}